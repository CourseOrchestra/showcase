<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="xsltforms/xsltforms.xsl" type="text/xsl"?>
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:xf="http://www.w3.org/2002/xforms">
  <head>
    <xf:model id="xformId_mainModel">
      <xf:instance id="xformId_mainInstance">
        <schema xmlns="">
        </schema>
      </xf:instance>
    </xf:model>
  </head>
  <body>
<!--    <xf:output value="serialize(instance('xformId_mainInstance'))"></xf:output>-->
    <div class="xformId_nameInput" style="float: left, clear: both, width: 350px">
        <div style="float: left, clear: both, width: 350px">Сформировать отчёт?</div>
    </div>
    <hr style="margin-top:21px; width: 100%;  float: left; clear: both;" />
    <div class="dirusing newbutton" style="width: 40%; float: left">
      <xf:trigger class="blue">
        <xf:label>Сформировать отчёт</xf:label>
        <xf:action ev:event="DOMActivate">
          <xf:load
            resource="javascript:gwtXFormDownload('xformId','reportDownload',  Writer.toString(getSubformInstanceDocument('xformId_mainModel', 'xformId_mainInstance')));gwtXFormUpdate('xformId','1', null)"
          ></xf:load>
        </xf:action>
      </xf:trigger>
    </div>
    <div class="dirusing newbutton" style="width: 40%; float: right">
      <xf:trigger class="blue">
        <xf:label>Закрыть</xf:label>
        <xf:action ev:event="DOMActivate">
          <xf:load resource="javascript:gwtXFormUpdate('xformId','1', null)"></xf:load>
        </xf:action>
      </xf:trigger>
    </div>
  </body>
</html>
