<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.3//EN"
"http://google-web-toolkit.googlecode.com/svn/tags/2.0.3/distro-source/core/src/gwt-module.dtd">

<module rename-to='secured'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->
  <inherits name='ru.beta2.extra.gwt.ExtraUI'/>
  <inherits name="com.google.gwt.http.HTTP"/>  
  
  <inherits name='com.sencha.gxt.ui.GXT'/>  
  
  <super-source path="jre"></super-source>

  <!-- Specify the app entry point class.                         -->
<!--     <entry-point class='ru.curs.showcase.app.client.App'/> -->

  <!-- Specify the paths for translatable code                    -->
  <source path='test'/>
  <source path='client'/>
  <source path='api'/>
  
  <public path="test/public"/>
  
  <stylesheet src="xsltforms/xsltforms.css" />
  <script src="xsltforms/xsltforms.js"></script>
  
  <stylesheet src="solutions/default/css/solution.css" />
  <stylesheet src="solutions/default/css/solutionGrid.css" />
    
<!--Для компиляции проекта требуется GWT версии 2.5.0-->        
<!--  <set-property name="user.agent" value="safari" /> -->
<!-- Должны быть включены ВСЕ агенты, иначе не запускаются GWT-тесты   -->    
<!-- для GridPanel в development mode во встроенном броузере       -->
       
<!--   The supported user agents at the moment of writing were:-->
<!--   ie6,ie8,gecko,gecko1_8,safari,opera	  -->
<!--   where safari==chrome, gecko1_8==firefox 2+ -->
<!-- Для того, чтобы компилировалось все - строчку set-property  -->
</module>
