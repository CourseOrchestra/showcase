<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="Curs.Def.Config" location="global.conf/Curs.Def.CheckStyle.config" type="project" description="">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="Curs.Def.Config" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
  <filter name="FileTypesFilter" enabled="true">
    <filter-data value="java"/>
  </filter>
  <filter name="FilesFromPackage" enabled="true">
    <filter-data value="src/ru/curs/showcase/app/jre"/>
    <filter-data value="src/ru/curs/showcase/test/ws"/>
    <filter-data value="datagrid"/>
  </filter>
  <filter name="NonSrcDirs" enabled="true"/>
</fileset-config>
